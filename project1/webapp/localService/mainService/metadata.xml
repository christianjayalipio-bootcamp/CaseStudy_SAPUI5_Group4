<edmx:Edmx Version="1.0" xmlns:edmx="http://schemas.microsoft.com/ado/2007/06/edmx" xmlns:sap="http://www.sap.com/Protocols/SAPData">
    <edmx:DataServices xmlns:m="http://schemas.microsoft.com/ado/2007/08/dataservices/metadata" m:DataServiceVersion="2.0">
        <Schema xmlns="http://schemas.microsoft.com/ado/2008/09/edm" Namespace="OrderModel">
            <EntityType Name="Order">
                <Key>
                    <PropertyRef Name="OrderNumber" />
                </Key>
                <Property Name="OrderNumber" Type="Edm.String" Nullable="false" sap:label="Order Number" />
                <Property Name="CreatedOn" Type="Edm.DateTime" Nullable="false" sap:label="Created On" />
                <Property Name="ReceivingPlant" Type="Edm.String" Nullable="false" sap:label="Receiving Plant" />
                <Property Name="DeliveringPlant" Type="Edm.String" Nullable="false" sap:label="Delivering Plant" />
                <Property Name="Status" Type="Edm.String" Nullable="false" sap:label="Status" />
                <Property Name="ProductName" Type="Edm.String" Nullable="false" sap:label="Product Name" />
                <Property Name="Quantity" Type="Edm.Int32" Nullable="false" sap:label="Quantity" />
                <Property Name="PricePerQuantity" Type="Edm.Decimal" Nullable="false" sap:label="Price Per Quantity" />
                <Property Name="TotalPrice" Type="Edm.Decimal" Nullable="false" sap:label="Total Price" />
            </EntityType>
 
            <EntityContainer Name="OrderEntities" m:IsDefaultEntityContainer="true">
                <EntitySet Name="Orders" EntityType="OrderModel.Order" />
            </EntityContainer>
        </Schema>
    </edmx:DataServices>
</edmx:Edmx>